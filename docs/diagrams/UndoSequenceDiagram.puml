@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":MassLinkersParser" as MassLinkersParser LOGIC_COLOR
participant "u:UndoCommand" as UndoCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":VersionedMassLinkers" as VersionedMassLinkers MODEL_COLOR
end box
[-> LogicManager : execute(undo)
activate LogicManager

LogicManager -> MassLinkersParser : parseCommand(undo)
activate MassLinkersParser

create UndoCommand
MassLinkersParser -> UndoCommand
activate UndoCommand

UndoCommand --> MassLinkersParser
deactivate UndoCommand

MassLinkersParser --> LogicManager : u
deactivate MassLinkersParser

LogicManager -> UndoCommand : execute()
activate UndoCommand

UndoCommand -> Model : undoMassLinkers()
activate Model

Model -> VersionedMassLinkers : undo()
activate VersionedMassLinkers

VersionedMassLinkers -> VersionedMassLinkers :resetData(ReadOnlyMassLinkers)
VersionedMassLinkers --> Model :
deactivate VersionedMassLinkers

Model --> UndoCommand
deactivate Model

UndoCommand --> LogicManager : result
deactivate UndoCommand
UndoCommand -[hidden]-> LogicManager : result
destroy UndoCommand

[<--LogicManager
deactivate LogicManager
@enduml
